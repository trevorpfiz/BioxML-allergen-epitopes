# Supabase
NEXT_PUBLIC_SUPABASE_URL=""
NEXT_PUBLIC_SUPABASE_ANON_KEY=""

# Config
PORT="3000"
NEXT_PUBLIC_ROOT_DOMAIN="biox-ml-allergen-epitopes.vercel.app"
NEXT_PUBLIC_SITE_URL="https://biox-ml-allergen-epitopes.vercel.app"

